{
  "name": "dice-counting",
  "version": "1.0.0",
  "description": "Count dice results",
  "main": "js/app.js",
  "author": "Jason Au",
  "license": "ISC",
  "scripts": {
    "start": "watchify -o js/bundle.js -v -d js/app.js",
    "build": "browserify . -t [envify --NODE_ENV production] | uglifyjs -cm > js/bundle.min.js"
  },
  "dependencies": {
    "react": "^0.14.5",
    "react-dom": "^0.14.5"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "envify": "^3.4.0",
    "reactify": "^1.1.1",
    "uglify-js": "^2.6.1",
    "watchify": "^3.6.1"
  },
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  }
}
